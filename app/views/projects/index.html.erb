<h1>Listing Projects</h1>

<div>
  <p id="notice"><%= notice %></p>
</div>


<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td> <%= link_to project.title %></td>
        <td><%= project.description %></td>
        <td><%= link_to 'Show', project_path(project) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
